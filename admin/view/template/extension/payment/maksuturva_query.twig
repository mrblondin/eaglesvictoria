{{ header }}
<div id="content">
  <div class="breadcrumb">
    {% for breadcrumb in breadcrumbs %}
    {{ breadcrumb.separator }}<a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a>
    {% endfor %}
  </div>
  <div class="box">
    <div class="heading">
      <h1><img src="view/image/payment.png" alt="" /> {{ heading_title }}</h1>
    </div>
    <div class="content">
    	{{ text_verifylist}}
	    <table class="list">
	    	<tr><td>{{ text_order}}</td><td>{{ text_message}}</td></tr>
	    	{% for id, message in statuses %}
	      <tr>
	        <td><a target="_blank" href="{{ links.id}}">#{{ id }}</a></td>
	        <td>{{ message}}</td>
	      </tr>
	      {% endfor %}
	    </table>
    </div>
  </div>
</div>
{{ footer }} 